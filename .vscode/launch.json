{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Next: Node",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/next",
            "cwd": "${workspaceFolder}",
            "env": {
                "NODE_OPTIONS": "--inspect",
            },
            "sourceMapPathOverrides": {
                "webpack:///*": "${workspaceFolder}/*"
            },
            "port": 9229,
            "console": "integratedTerminal"
        },
    ]
}
